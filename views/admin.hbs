<div class="container">
<a href="/createuser" class="btn btn-success btn-md my-3 ms-auto">Create User</a>
<h3 class="text-center">USER DATA</h3>
<table class="table ">
  <thead>
    <tr>
        
      <th scope="col">Uaer id</th>
      <th scope="col">User Name</th>
      <th scope="col">User Email</th>
      
    </tr>
  </thead>
  <tbody>
      {{#each users}}
       <tr>
        {{#with this}}
      <td>{{_id}}</td>
      <td>{{name}}</td>
      <td>{{email}}</td>
      <td><a href="/adminaction/edit/{{_id}}" class="btn btn-primary">Edit</a></td>
      <td><a href="/adminaction/{{_id}}" class="btn btn-danger" onclick="return confirm('Do you want to delete {{name}}?')">Delete</a></td>
      {{/with}}
      </tr>
      {{/each}}

      </div>



